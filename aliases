alias gaa="git add ."
alias ga="git add"
alias gcm="git commit -m"
alias gco="git checkout"
alias gb="git branch"
alias gm="git merge"
alias gp="git push"
alias gl="git pull"
alias all-permission="sudo chmod -R 777"
alias art="php artisan"

#delete local branches except stage and your current branch
alias del_lb="git branch | grep -vE '(stage|\*)' | xargs git branch -D"

#switch between php versions
alias sw_php="sudo update-alternatives --config php"
